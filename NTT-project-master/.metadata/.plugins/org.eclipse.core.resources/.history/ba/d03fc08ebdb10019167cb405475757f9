typedef struct ImuData {
	float AccXData;
	float AccYData;
	float AccZData;
	float Temp;
	float GyroXData;
	float GyroYData;
	float GyroZData;
	float MagXData;
	float MagYData;
	float MagZData;
}IMU_tstImuData;

